            <Border Grid.Row="0" Grid.Column="7" BorderBrush="White" BorderThickness="1" Margin="2" Padding="2,2,2,2">
                <TextBlock x:Name="Select8" Text="8" FontSize="18" HorizontalAlignment="Center" Tapped="BlockTapped"/>
            </Border>
            <Border Grid.Row="0" Grid.Column="8" BorderBrush="White" BorderThickness="1" Margin="2" Padding="2,2,2,2">
                <TextBlock x:Name="Select9" Text="9" FontSize="18" HorizontalAlignment="Center" Tapped="BlockTapped"/>
            </Border>
            <Border Grid.Row="0" Grid.Column="9" BorderBrush="White" BorderThickness="1" Margin="2" Padding="2,2,2,2">
                <TextBlock x:Name="Select0" Text="X" FontSize="18" HorizontalAlignment="Center" Tapped="BlockTapped"/>
            </Border>
        </Grid>
        
        <Grid Grid.Row="4" Grid.Column="0" Grid.ColumnSpan="3" Padding="5,5,5,5">
            <CheckBox x:Name="PencilMarks" Content="Pencil Marks" HorizontalAlignment="Center"/>
        </Grid>

        <Grid Grid.Row="5" Grid.Column="0" Grid.ColumnSpan="3" Padding="5,5,5,5">
            <TextBlock x:Name="StatusMessage" Text="" HorizontalAlignment="Center"/>
        </Grid>

        <Grid Grid.Row="6" Grid.Column="0" Grid.ColumnSpan="3" Padding="5,5,5,5">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <Button Grid.Row="0" Grid.Column="0" Content="New" HorizontalAlignment="Center" Tapped="NewButton"/>
            <Button Grid.Row="0" Grid.Column="1" Content="Solve" HorizontalAlignment="Center" Tapped="SolveButton"/>
        </Grid>
    </Grid>
</Page>
